{
  "name": "google-play-scraper",
  "version": "10.0.1",
  "description": "scrapes app data from google play store",
  "type": "module",
  "exports": {
    "import": "./index.js",
    "require": "./dist/cjs/index.js"
  },
  "main": "./dist/cjs/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "mocha --timeout 5000",
    "lint": "eslint .",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "node scripts/copy-esm.cjs",
    "build:cjs": "babel lib --out-dir dist/cjs/lib --config-file ./babel.config.json && babel index.js --out-dir dist/cjs --config-file ./babel.config.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/facundoolano/google-play-scraper"
  },
  "keywords": [
    "google",
    "play",
    "scraper"
  ],
  "author": "Facundo Olano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/facundoolano/google-play-scraper/issues"
  },
  "homepage": "https://github.com/facundoolano/google-play-scraper",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "debug": "^3.1.0",
    "got": "^11.8.6",
    "memoizee": "^0.4.14",
    "ramda": "^0.29.0",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.0",
    "@babel/core": "^7.22.0",
    "@babel/plugin-transform-runtime": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/runtime": "^7.22.0",
    "chai": "^4.3.7",
    "eslint": "^8.46.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "fs-extra": "^11.3.2",
    "mocha": "^10.2.0",
    "promise-log": "^0.1.0",
    "sinon": "^15.2.0",
    "validator": "^13.9.0"
  }
}
